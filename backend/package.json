{
  "type": "module",
  "name": "location service",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l error app.js",
    "dev": "fastify start -w -l info -P app.js",
    "pretest": "standard",
    "lint": "standard --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "standard": {
    "envs": [
      "mocha"
    ],
    "globals": [
      "XPathResult",
      "$",
      "jQuery",
      "fetch"
    ],
    "ignore": [
      "node_modules/**",
      "**/_*"
    ]
  },
  "dependencies": {
    "@fastify/autoload": "^5.5.0",
    "@fastify/basic-auth": "^4.0.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/mongodb": "^6.1.0",
    "@fastify/sensible": "^5.1.1",
    "@fastify/swagger": "^7.6.1",
    "fastify": "^4.10.0",
    "fastify-cli": "^5.6.0",
    "fastify-plugin": "^4.3.0",
    "node-fetch": "^3.3.0",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "0.2.0",
    "standard": "^17.0.0",
    "tap": "^16.3.2"
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--experimental-loader",
      "@istanbuljs/esm-loader-hook"
    ]
  }
}
